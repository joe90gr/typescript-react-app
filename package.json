{
  "name": "koa-typescript-babel",
  "version": "1.0.0",
  "description": "simple koa typescript app setup",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --out-dir dist --extensions '.ts,.tsx' --copy-files",
    "build:dev": "babel ./src --out-dir dist --extensions '.ts,.tsx' --source-maps --watch --copy-files",
    "webpack": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc --watch",
    "start": "node ./dist/createSite.server",
    "start:dev": "nodemon ./dist/createSite.server"
  },
  "author": "Giuseppe Ruggieri",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/koa": "^2.11.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.4",
    "@types/koa__router": "^8.0.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@koa/router": "^8.0.6",
    "koa": "^2.11.0",
    "koa-hbs": "^0.9.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-router-dom": "^5.1.2"
